{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "strict": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "listEmittedFiles": true,
    "esModuleInterop": true,

    "outDir": "./result/tsc",
    "baseUrl": ".",
    "paths": {
      "@routes/*": ["src/routes/*"]
    }
  },

  "include": ["src/**/*"]
}
